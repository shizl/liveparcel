<?php

/**
 * @file
 * Install, update, and uninstall functions for the uc_store module.
 */

/**
 * Implements hook_requirements().
 */
function lp_parcel_requirements($phase) {
  $requirements = array();

  if ($phase == 'runtime') {
    $severities = array(
      'ok' => REQUIREMENT_OK,
      'warning' => REQUIREMENT_WARNING,
      'error' => REQUIREMENT_ERROR,
    );
    module_load_include('inc', 'lp_parcel');
    $results = module_invoke_all('liveparcels_status');
    foreach ($results as $status) {
      $requirements[] = array(
        'severity' => isset($severities[$status['status']]) ? $severities[$status['status']] : REQUIREMENT_INFO,
        'title' => $status['title'],
        'value' => $status['desc'],
      );
    }
  }

  return $requirements;
}

/**
 * Implements hook_uninstall().
 */
function lp_parcel_uninstall() {
    db_query("DELETE FROM `field_config` WHERE `field_name` IN ( SELECT `field_name` FROM `field_config_instance` WHERE `bundle`='live_parcel')");
   db_query('DROP TABLE `field_data_field_authority_to_leave`, `field_data_field_cash_to_collect`, `field_data_field_customer_reference`, `field_data_field_delivery_note`, `field_data_field_parcel_delivery_address`, `field_data_field_parcel_order_id`, `field_data_field_parcel_pickup_address`, `field_data_field_parcel_price`, `field_data_field_parcel_size`, `field_data_field_parcel_weight`, `field_data_field_pickup_note`, `field_data_field_pick_up_time`, `field_data_field_pick_up_time_ampm`, `field_revision_field_authority_to_leave`, `field_revision_field_cash_to_collect`, `field_revision_field_customer_reference`, `field_revision_field_delivery_note`, `field_revision_field_parcel_delivery_address`, `field_revision_field_parcel_order_id`, `field_revision_field_parcel_pickup_address`, `field_revision_field_parcel_price`, `field_revision_field_parcel_size`, `field_revision_field_parcel_weight`, `field_revision_field_pickup_note`, `field_revision_field_pick_up_time`, `field_revision_field_pick_up_time_ampm`;');
   db_delete('field_config_instance')->condition('bundle', 'live_parcel', '=')->execute();
   db_delete('node_type')
      ->condition('type', 'live_parcel', '=')
    ->execute();
}
